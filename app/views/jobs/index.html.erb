<h1>Listing jobs</h1>

<table>
  <thead>
    <tr>
      <th>Description</th>
      <th>Starttime</th>
      <th>Priority</th>
      <th>Part Number</th>
      <th>Like</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @jobs.each do |job| %>
      <tr>
        <td><%= job.description %></td>
        <td><%= job.starttime %></td>
        <td><%= job.priority %></td>
        <td><%= job.part_number %></td>
        <td><%= job.like %></td>
        <td><%= link_to 'Show', job %></td>
        <td><%= link_to 'Edit', edit_job_path(job) %></td>
        <td><%= link_to 'Destroy', job, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Job', new_job_path %>
